{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ],
        "version": "sp-typescript-latest",
        "src": ".",
        "dest": "api"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "version": "sp-typescript-latest",
        "dest": "api"
      }
    ],
    "versions": {
      "sp-typescript-latest": {
        "dest": "sp-typescript-latest"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "apiPlatform":"javascript",
      "extendBreadcrumb": "true",
      "breadcrumb_path":"/javascript/api/breadcrumb/toc.json",
        "ms.suite": "office365",
	      "ms.author": "spdevdocs",
	      "author": "spdevdocs",
	      "ms.topic": "reference"
    },
    "fileMetadata": {
    	"monikers":
      {
          "**/*.yml": [ "sp-typescript-latest" ]
      },
	"ms.service": {
		"SP-Framework/**.yml": "sharepoint"
	},
	"ms.subservice":{
		"SP-Framework/**.yml": "sharepoint-framework"
	}
    },
    "template": [],
    "dest": "SP-Framework"
  }
}
